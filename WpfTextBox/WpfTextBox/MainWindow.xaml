<Window x:Class="WpfTextBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox  Name="TextBoxTest" HorizontalAlignment="Left" Height="23" Margin="100,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="150"/>
        <WrapPanel HorizontalAlignment="Left" DockPanel.Dock="Top" Margin="100,100,0,10">
            <Button Name="Button" Click="Button_Clicked">Print</Button>
        </WrapPanel>
    </Grid>
</Window>
